{
  "extends": "airbnb/base",
  "rules": {
    "camelcase": [0, { "properties": "always" }],
    "max-len": [1, {"code": 150, "tabWidth": 2, "ignoreUrls": true}],
    "arrow-body-style": 0,
    "consistent-return": 1,
    "no-fallthrough": 1,
    "no-useless-constructor": 0,
    "no-param-reassign": 0,
    "no-console": 0,
    "no-process-exit": 0,
    "no-extra-boolean-cast": 0,
    "no-underscore-dangle": 0,
    "semi": ["error", "never"],
    "func-names": 0,
    "class-methods-use-this": 0,
    "global-require": 0
  },
  "globals": {
    "describe": true,
    "it": true,
    "before": true,
    "after": true,
    "beforeEach": true,
    "afterEach": true
  }
}
